  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.AspNetCore.Hosting": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./wwwroot/logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "JWT": {
    "SignInKey": "MonstrsIncMonstrsIncMonstrsIncMonstrsIncMonstrsIncMonstrsIncMonstrsIncMonstrsIncMonstrsInc"
  } 
}
